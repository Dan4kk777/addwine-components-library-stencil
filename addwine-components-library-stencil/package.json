{
  "name": "addwine-components-library-stencil",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/addwine-components-library-stencil/addwine-components-library-stencil.esm.js",
  "exports": {
    ".": {
      "import": "./dist/addwine-components-library-stencil/addwine-components-library-stencil.esm.js",
      "require": "./dist/addwine-components-library-stencil/addwine-components-library-stencil.cjs.js"
    },
    "./my-component": {
      "import": "./dist/components/my-component.js",
      "types": "./dist/components/my-component.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stenciljs/component-starter.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^7.0.2",
    "@stencil/core": "^4.27.1",
    "@stencil/postcss": "^2.1.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "autoprefixer": "^10.4.21",
    "cssnano": "^7.0.7",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "postcss": "^8.5.5",
    "postcss-replace": "^2.0.1",
    "puppeteer": "^24.3.0",
    "tailwindcss": "^3.4.17"
  },
  "license": "MIT"
}
